{
  "name": "@or-q/workspace",
  "version": "1.0.0",
  "private": true,
  "description": "OR-Q Workspace Package",
  "scripts": {
    "pretest": "pnpm run lint",
    "test": "true",
    "lint:eslint": "eslint",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "lint:markdownlint": "markdownlint --ignore '**/node_modules/**/*' '**/*.md'",
    "lint": "pnpm run lint:tsc && pnpm run lint:eslint && pnpm run lint:prettier && pnpm run lint:markdownlint",
    "fix:eslint": "eslint --fix",
    "fix:prettier": "prettier --write .",
    "fix:markdownlint": "markdownlint --ignore '**/node_modules/**/*' --fix '**/*.md'",
    "fix": "pnpm run fix:eslint && pnpm run fix:prettier && pnpm run fix:markdownlint",
    "lefthook:precommit": "pnpm exec lefthook run pre-commit"
  },
  "keywords": [
    "or-q"
  ],
  "author": "Alexander Gladysh <agladysh@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@or-q/cli": "workspace:*",
    "@or-q/plugin-core": "workspace:*",
    "@or-q/plugin-dot-or-q-assets": "workspace:*",
    "@or-q/plugin-fetch": "workspace:*",
    "@or-q/plugin-filesystem": "workspace:*",
    "@or-q/plugin-format": "workspace:*",
    "@or-q/plugin-jp": "workspace:*",
    "@or-q/plugin-macro": "workspace:*",
    "@or-q/plugin-openrouter-api": "workspace:*",
    "@or-q/plugin-ollama": "workspace:*",
    "@or-q/plugin-shell": "workspace:*",
    "@or-q/plugin-stdio-logger": "workspace:*",
    "@or-q/plugin-store": "workspace:*",
    "@or-q/plugin-template": "workspace:*",
    "@or-q/plugin-yaml-script": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "lefthook": "^1.12.2",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
